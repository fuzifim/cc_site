{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.5.*",
		"laravelcollective/html": "5.5.*",
        "symfony/dom-crawler": "3.1.*",
        "symfony/css-selector": "3.1.*",
        "laravel/socialite": "3.0.0",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "jenssegers/agent": "^2.3",
        "adamwathan/eloquent-oauth-l5" : "^0.5.2",
        "roumen/sitemap": "^2.5",
        "facebook/graph-sdk": "v5.*",
        "laravelcollective/remote": "5.0",
		"dawson/youtube": "1.1.0-beta",
		"google/recaptcha": "~1.1",
		"greggilbert/recaptcha": "dev-master",
        "darryldecode/cart": "~3.0",
		"facuz/laravel-themes": "^3.1",
		"zizaco/entrust": "5.2.x-dev", 
		"Nathanmac/Parser": "4.*",
		"jeremykendall/php-domain-parser": "^5.3",
        "pbmedia/laravel-ffmpeg": "^1.1",
        "graham-campbell/digitalocean": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
		"willdurand/geocoder": "@stable",
        "kriswallsmith/buzz": "@stable",
        "php-ffmpeg/php-ffmpeg": "^0.11.0",
        "alexpechkarev/google-maps": "1.0.9",
        "lullabot/amp": "^1.0.0", 
		"donatello-za/rake-php-plus": "^1.0",
		"gufy/cpanel-php":"~1.0",
		"elasticquent/elasticquent": "dev-master",
        "appsketch/adfly": "^1.1",
		"league/flysystem-aws-s3-v3": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "phpspec/phpspec": "~2.1",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database",
			"app/Libraries",
            "app/WebService"
        ],
        "psr-4": {
            "App\\": "app/"
        },
		"files": [
			"app/Libraries/Helpers.php"
		]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
            
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
          "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
          "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
